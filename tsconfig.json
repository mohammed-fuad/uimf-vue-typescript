{
  "compilerOptions": {
    "lib": [
      "dom",
      "es5",
      "es6",
      "es2015",
      "es2015.promise"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src/",
    "paths": {
      "core-form": [
        "./components/form/index"
      ],
      "core-ui/*": [
        "./core/ui/*"
      ],
      "core-framework": [
        "./core/framework/index"
      ],
      "core-handlers": [
        "./core/handlers/index"
      ],
      "core-eventHandlers": [
        "./core/eventHandlers/index"
      ],
      "core-functions": [
        "./core/functions/index"
      ]
    }
  }
}